<div id="MenuIcon">
    <div id="MenuLine"></div>
</div>

<div id="map"></div><br>

<div class="container">
  <div>
    <h1>All Events</h1>
    {{#each events}}
    <div class="panel panel-default">
      <div class="panel-heading">
        <h3 class="panel-title">{{this.activity}}</h3>
      </div>
      <div class="panel-body">
        {{this.date}}<br>
        {{this.location}}<br>
        {{this.description}}<br>
      </div>
    {{/each}}
    </div>
  </div>
</div>


<div id="MainMenu">
  <br><br><br>
    <div id="logo"><img src="assets/images/resqqssff.png" height="75px" width="75px"/></div>
    <ul>
        <li><a id="profile" href="/profile">Profile</a><div class="line"></div></li>
        <li><a href="/dashboard">View Map</a><div class="line"></div></li>
        <li><a href="user/:id/event/new">Create Event</a><div class="line"></div></li>
        <li><a href="user/:id/events">View my Events</a><div class="line"></div></li>
        <li>Add an issue (coming Soon)<div class="line"></div></li>
        <li>Logout<div class="line"></div></li>
        <li><div id="close"><img src="assets/images/close.png" height="30px" width="30px"/></div></li>
    </ul>

</div>

